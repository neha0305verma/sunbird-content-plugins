{
    "id": "org.ekstep.questionunit.subjective",
    "ver": "1.0",
    "author": "Jagadish",
    "title": "Subjective Question Unit Plugin",
    "description": "Question Unit Plugin that enables creation of subjective questions.",
    "publishedDate": "",
    "languages": [
        "English"
    ],
    "categories": [],
    "keywords": [
        "subjectiveplugin"
    ],
    "targets": [{
        "id": "org.ekstep.questionset",
        "ver": 1.0
    }],
    "editor": {
        "main": "editor/plugin.js",
        "dependencies": [{
                "type": "plugin",
                "plugin": "org.ekstep.questionunit",
                "ver": "1.1"
            },
            {
                "type": "js",
                "src": "editor/controllers/subjective-controller.js"
            },
            {
                "type": "css",
                "src": "editor/styles/style.css"
            }
        ]
    },
    "templates": [{
        "id": "horizontalMCQ",
        "thumbnail": "editor/assets/mcq-horizontal.png",
        "title": "Subjective Question",
        "disc": "Subjective items consist of a stem. The stem is the beginning part of the item that presents the item as a problem to be solved, a question asked of the respondent, or an incomplete statement to be completed, as well as any other relevant information.",
        "category": "SUBJECTIVE",
        "editor": {
            "templateURL": "editor/templates/subjective_template.html",
            "controllerURL": "editor/controllers/subjective-controller.js",
            "template": "subjective",
            "controller": "QuestionFormController"
        }
    }],
    "renderer": {
        "main": "renderer/plugin.js",
        "dependencies": [{
                "type": "css",
                "src": "renderer/styles/style.css"
            },
            {
                "type": "css",
                "src": "renderer/styles/horizontal_and_vertical.css"
            },
            {
                "type": "js",
                "src": "renderer/template/mcq-layouts.js"
            },
            {
                "type": "js",
                "src": "renderer/template/template_controller.js"
            },
            {
                "type": "image",
                "src": "renderer/assets/tick_icon.png",
                "assetId": "org.ekstep.questionunit.mcq.tick_icon.png"
            },
            {
                "type": "image",
                "src": "renderer/assets/audio-icon2.png",
                "assetId": "org.ekstep.questionunit.mcq.audio-icon2.png"
            },
            {
                "type": "image",
                "src": "renderer/assets/music-blue.png",
                "assetId": "org.ekstep.questionunit.music-blue.png"
            }
        ]
    },
    "dependencies": [{
        "plugin": "org.ekstep.questionunit",
        "ver": "1.1",
        "type": "plugin",
        "scope": "all"
    }]
}